{
  "name": "awlds-ui-lib",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/awlds-ui-lib/awlds-ui-lib.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlightEngineering/awlds-ui-lib.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "plop"
    ]
  },
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --docs --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "storybook": "start-storybook -p 6006 -c .storybook -s www",
    "build-storybook": "build-storybook -c .storybook -o docs-build",
    "generate.component": "plop",
    "story": "npm-run-all --sequential build-storybook --parallel build.watch storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "@stencil/sass": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@ljcl/storybook-addon-cssprops": "^2.3.2",
    "@storybook/addon-a11y": "^6.5.6",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/addon-storyshots": "^6.5.6",
    "@storybook/builder-webpack4": "^6.5.6",
    "@storybook/html": "^6.5.6",
    "@storybook/manager-webpack4": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@types/jest": "^27.0.3",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "nanoid": "^3.3.4",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.0",
    "puppeteer": "^10.0.0",
    "vanilla-masker": "^1.2.0"
  },
  "license": "MIT"
}
